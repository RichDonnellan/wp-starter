{
    "name": "aac/alcohol-org",
    "type": "project",
    "description":
        "Custom theme for Alcohol.org",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=7.0",
        "composer/installers": "^1.5",
        "wpackagist-plugin/wordpress-seo":"7.0.2",
        "wpackagist-plugin/wordfence":"7.1.3"
    },
    "require-dev": {
        "wp-cli/wp-cli": "^1.5"
    },
    "extra": {
        "installer-paths": {
            "../../mu-plugins/{$name}/": ["type:wordpress-muplugin"],
            "../../plugins/{$name}/": ["type:wordpress-plugin"]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "bash post-install.sh"
        ],
        "post-update-cmd": [
            "bash post-install.sh"
        ]
    }
}
